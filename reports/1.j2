                        <circle r="5" cx="10" cy="10" fill="lightgray" />

                        {% set total = data | length %}
                        {% set circumference = 31.4159 %}

                        {% set count_0 = data | selectattr("NATURE", "equalto", [0]) | list | length %}
                        
                        {% set prop_0 = count_0 / total %}
                       
                        {% set dash_0 = prop_0 * circumference %}

                        {% set angle_0 = -90 %}
                        
                        <circle r="5" cx="10" cy="10"
                            fill="transparent"
                            stroke="midnightblue"
                            stroke-width="10"
                            stroke-dasharray="{{ dash_0 }} {{ circumference }}"
                            transform="rotate({{ angle_0 }} 10 10)" />   
