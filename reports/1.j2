        </colgroup>
        
        {%set count = namespace(taiidNashode=0)%}
        {%for contract in data%}
            {% for inv in contract.INVS | list %}
                {% if inv.SUM_CNFRMED_EMPLYR is not defined or inv.SUM_CNFRMED_EMPLYR in [None, [], 0] %}
                    {% set count.taiidNashode = count.taiidNashode + 1 %}
                {% endif %}
            {% endfor %}
        {%endfor%}

        {#شمارش تعداد کل صورت وضعیت ها#}
        {%set count2 = namespace(kol=0)%}
        {%for contract in data%}
            {% for inv in contract.INVS | list %}
                    {% set count2.kol = count2.kol + 1 %}
            {% endfor %}
        {%endfor%}
        <tr>
            <td style="text-align:right">تعداد کل صورت وضعیت های ارسالی:</td>
            <td style="text-align:right; border-right-style: hidden; "><span style="font-size:12; background-color: lightgray">{{count2.kol}}</span> عدد</td>
        </tr>
        <tr>
            <td style="text-align:right">تعداد کل صورت وضعیت های تاییدی:</td>
            <td style="text-align:right; border-right-style: hidden;"><span style="font-size:12; background-color: lightgray;">{{count2.kol - count.taiidNashode }}</span> عدد</td>
        </tr> 
