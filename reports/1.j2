                                                                            | map(attribute="0") 
                                                                            | selectattr("amount","defined") 
                                                                            | map(attribute='amount') 
                                                                            | list | sum | digit2word }} ریال <span></td>
                                                                                    
        </tr>

        <tr>
        {%if data | selectattr("TOTAL_AMNT_INV_SENT", "defined") | map(attribute="TOTAL_AMNT_INV_SENT") | selectattr("0","defined") | map(attribute="0") | selectattr("amount","defined") | map(attribute='amount') | list | sum%}
            <td colspan="2" style="text-align:right; background-color:white" colspan="2"><u>{{((data | selectattr("TOTAL_INV_CNFRMED", "defined") | map(attribute="TOTAL_INV_CNFRMED") | selectattr("0","defined") | map(attribute="0") | selectattr("amount","defined") | map(attribute='amount') | list | sum ) / (data | selectattr("TOTAL_AMNT_INV_SENT", "defined") | map(attribute="TOTAL_AMNT_INV_SENT") | selectattr("0","defined") | map(attribute="0") | selectattr("amount","defined") | map(attribute='amount') | list | sum) * 100) | int }}%</u> مبلغ صورت وضعیت های ارسالی، تایید شده اند.</td>
        {%endif%}
        </tr>

    
        <tr>
            <td colspan="2" style= "background-color: #d6d2d2;"></td>
